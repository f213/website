{
  "name": "blog",
  "version": "1.0.0",
  "description": "borshev.com website",
  "author": "Fedor Borshev",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --max-warnings 0 --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "healthcheck": "node /healthcheck.js borshev.com 3000 / 200"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/proxy": "^2.0.1",
    "@nuxtjs/redirect-module": "^0.3.1",
    "@nuxtjs/sentry": "^4.4.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "bulma": "^0.9.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.15.4",
    "nuxt-jsonld": "^1.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-vue": "^7.1.0",
    "javascript-time-ago": "^2.0.8",
    "jest": "^26.6.3",
    "lodash.throttle": "^4.1.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.2",
    "postcss-nested": "^4.1.0",
    "prettier": "^2.3.2",
    "sass-loader": "^10.0.5",
    "vue-jest": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    },
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
